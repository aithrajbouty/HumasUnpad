        android:supportsRtl="true"
        android:theme="@style/Theme.JurnalistikUnpad">
        <activity android:name=".SejarahActivity"></activity>
        <activity android:name=".OrganisasiActivity" />
        <activity android:name=".TujuanActivity" />
        <activity android:name=".VisimisiActivity" />
        <activity android:name=".DosenActivity" />
        <activity android:name=".JurnalActivity" />
        <activity android:name=".KurikulumActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>